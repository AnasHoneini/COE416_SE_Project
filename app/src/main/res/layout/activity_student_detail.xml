<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">

    <FrameLayout android:id="@+id/frameStudentDetail"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyledeep_orange_A400_1">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGrade6Secti"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_55pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_weight="0.79"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        style="@style/txtInterextrabold18_1" tools:text="@string/msg_grade_6_secti"/>
                    <ImageView android:id="@+id/imageMaximize"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginStart="@dimen/_55pxh"
                        android:layout_width="@dimen/_22pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_maximize"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_maximize"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowellipsefive"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_57pxv"
                android:layout_marginEnd="@dimen/_9pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageEllipseFive"
                    android:layout_height="@dimen/_128pxh"
                    android:layout_width="@dimen/_129pxh"
                    android:scaleType="centerCrop"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse5"/>
                <TextView android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_weight="0.62"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_7pxh"
                    style="@style/txtInterextrabold18_1" tools:text="@string/msg_name_joe_james"/>
            </LinearLayout>
        </LinearLayout>
        <View
            android:background="@drawable/rectangle_bg_white_a700_radius_20"
            android:id="@+id/viewRectangleFifteen"
            android:layout_gravity="bottom|start"
            android:layout_height="@dimen/_550pxh"
            android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_360pxh"/>
        <LinearLayout android:id="@+id/linearColumndescriptionOne"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_2">
            <TextView android:gravity="start"
                android:id="@+id/txtDescriptionOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_23pxh"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_7pxh"
                style="@style/txtInterextrabold18_2"
                textUnderline="true" tools:text="@string/msg_today_jan_23"/>
            <LinearLayout android:id="@+id/linearColumnincident"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyledeep_orange_A100">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowincident"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtIncident"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.62"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        style="@style/txtIntersemibold16" tools:text="@string/lbl_incident"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_44pxh"
                        android:layout_weight="0.38"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        style="@style/txtIntersemibold16" tools:text="@string/lbl_time"/>
                </LinearLayout>
                <View android:background="@color/gray_600"
                    android:id="@+id/lineLineFive"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_22pxv" android:layout_width="@dimen/_327pxh"/>
                <LinearLayout android:id="@+id/linearRow"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_4pxh"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/linearRowfightwithafr"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.67"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <TextView android:id="@+id/txtFightwithafr"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.55"
                            android:layout_width="0dp"
                            style="@style/txtInterregular16" tools:text="@string/msg_fight_with_a_fr"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTimeOne"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_14pxv"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.45"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            style="@style/txtInterregular16" tools:text="@string/lbl_10_00_am"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowfeedback"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginStart="@dimen/_50pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_weight="0.33"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFeedback"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.92"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            style="@style/txtInterbold14"
                            textUnderline="true" tools:text="@string/lbl_feedback"/>
                        <ImageView android:id="@+id/imageArrowright"
                            android:layout_height="@dimen/_8pxh"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_14pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="@dimen/_5pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowright_gray_902"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_gray_902"/>
                    </LinearLayout>
                </LinearLayout>
                <View android:background="@color/gray_600"
                    android:id="@+id/lineLineSix"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_15pxv" android:layout_width="@dimen/_327pxh"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowfightwithafrOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_4pxh"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:id="@+id/txtFightwithafrOne"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.38"
                        android:layout_width="0dp"
                        style="@style/txtInterregular16" tools:text="@string/msg_fight_with_a_fr"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTimeTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.3"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        style="@style/txtInterregular16" tools:text="@string/lbl_12_30_pm"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFeedbackOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginStart="@dimen/_48pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.29"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        style="@style/txtInterbold14"
                        textUnderline="true" tools:text="@string/lbl_feedback"/>
                    <ImageView android:id="@+id/imageArrowrightOne"
                        android:layout_height="@dimen/_8pxh"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="@dimen/_5pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright_gray_902"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_gray_902"/>
                </LinearLayout>
                <View android:background="@color/gray_600"
                    android:id="@+id/lineLineSeven"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_327pxh"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowtotalperday"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_152pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTotalperday"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.91"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        style="@style/txtInterbold16_1" tools:text="@string/lbl_total_per_day"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_43pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.09"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        style="@style/txtInterbold16_1" tools:text="@string/lbl_22"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowleftOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleftOne"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft_black_900_12x14"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_black_900_12x14"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_weight="0.31"
                    android:layout_width="0dp" android:singleLine="true"
                    style="@style/txtInterbold14" textUnderline="true" tools:text="@string/lbl_view_history"/>
                <LinearLayout android:id="@+id/linearRowcontactparent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_weight="0.64"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStyledeep_orange_A400cornerRadius">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtContactParent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_30pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_weight="0.82"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        style="@style/txtInterbold14_2"
                        textUnderline="true" tools:text="@string/lbl_contact_parent"/>
                    <ImageView android:id="@+id/imageArrowrightTwo"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginEnd="@dimen/_19pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="@dimen/_22pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
