<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:andriod="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameLoginPage"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    tools:context=".ActivityStudentList">

    <LinearLayout
        android:id="@+id/linearRowarrowleft"
        style="@style/groupStyledeep_orange_A400_d6"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_90pxh"
        android:layout_marginBottom="-13dp">

        <ImageView
            android:id="@+id/imageArrowleft"
            android:layout_width="@dimen/_24pxh"
            android:layout_height="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_marginBottom="@dimen/_94pxv"
            android:onClick="goBackToParent"
            android:scaleType="fitXY"
            android:src="@drawable/img_arrowleft"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/txtGrade6Secti"
            style="@style/txtInterextrabold18_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_55pxh"
            android:layout_marginTop="@dimen/_34pxv"
            android:ellipsize="end"
            android:lineSpacingExtra="@dimen/_4pxh"
            android:singleLine="true"
            android:text="@string/msg_grade_6_secti" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearColumnlogin"
        style="@style/groupStylewhite_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearRowarrowleft"
        android:layout_gravity="top|center"
        android:layout_marginStart="@dimen/_16pxh"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="@dimen/_16pxh"
        android:layout_marginBottom="58dp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearRowlogin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_36pxh"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/students"
                style="@style/txtInterextrabold12_2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.44"
                android:background="@android:color/transparent"
                android:ellipsize="end"
                android:onClick="goToStudentList"
                android:singleLine="true"
                android:text="Students"
                tools:ignore="TextSizeCheck,TextSizeCheck" />


            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/report"
                style="@style/txtInterextrabold12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_70pxh"
                android:layout_weight="0.44"
                android:background="@android:color/transparent"
                android:ellipsize="end"
                android:onClick="goToReport"
                android:singleLine="true"
                android:text="Report"
                tools:ignore="TextSizeCheck" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/txtTaptoviewins"
        style="@style/txtInterbold14"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_4pxh"
        android:layout_marginTop="192dp"
        android:layout_marginEnd="@dimen/_4pxh"
        android:ellipsize="end"
        android:gravity="center_horizontal"
        android:singleLine="true"
        android:text="@string/msg_tap_to_view_ins" />


    <EditText
        android:id="@+id/btnSearchStudentName"
        style="@style/btnSolidRoundedOutline"
        android:layout_width="164dp"
        android:layout_height="37dp"
        android:layout_below="@+id/txtTaptoviewins"
        android:layout_marginStart="20dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="202dp"
        android:ellipsize="end"
        android:gravity="center"
        android:singleLine="true"
        android:text="@string/msg_search_student"
        android:textAllCaps="false" />

    <EditText
        android:id="@+id/btnMmddyyyy"
        style="@style/btnSolidRoundedOutline"
        android:layout_width="164dp"
        android:layout_height="37dp"
        android:layout_below="@+id/txtTaptoviewins"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="@dimen/_54pxh"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="22dp"
        android:ellipsize="end"
        android:gravity="center"
        android:lineSpacingExtra="@dimen/_3pxh"
        android:singleLine="true"
        android:text="@string/lbl_mm_dd_yyyy"
        android:textAllCaps="false" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_249pxh">

        <ListView
            android:id="@+id/namesListView"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />


    </RelativeLayout>

</RelativeLayout>
