<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="studentFeedbackOneVM" type="com.se_application.app.modules.studentfeedbackone.data.viewmodel.StudentFeedbackOneVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStudentFeedbac"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearRowarrowleft"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyledeep_orange_A400_1">
            <ImageView android:id="@+id/imageArrowleft"
                android:layout_height="@dimen/_16pxh"
                android:layout_marginBottom="@dimen/_208pxv"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_42pxv"
                android:layout_width="@dimen/_24pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumngrade6secti"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_75pxv"
                android:layout_marginStart="@dimen/_55pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_weight="0.79" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtGrade6Secti"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{studentFeedbackOneVM.studentFeedbackOneModel.txtGrade6Secti}"
                    style="@style/txtInterextrabold18_1" tools:text="@string/msg_grade_6_secti"/>
                <ImageView android:id="@+id/imageEllipseFive"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_90pxh"
                    android:layout_marginEnd="@dimen/_37pxh"
                    android:layout_marginStart="@dimen/_38pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="@dimen/_95pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse5}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse5"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtJoeJames"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_37pxh"
                    android:layout_marginStart="@dimen/_38pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{studentFeedbackOneVM.studentFeedbackOneModel.txtJoeJames}"
                    style="@style/txtInterextrabold18_1" tools:text="@string/lbl_joe_james"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageMaximize"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginBottom="@dimen/_220pxv"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_55pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="@dimen/_22pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_maximize"
                tools:ignore="ContentDescription" tools:src="@drawable/img_maximize"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearColumnreportstudent"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_2">
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtReportStudent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_38pxh"
                android:layout_marginStart="@dimen/_41pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:text="@{studentFeedbackOneVM.studentFeedbackOneModel.txtReportStudent}"
                style="@style/txtInterbold20" tools:text="@string/msg_report_student"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumndate"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyledeep_orange_A100">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowdate"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDate"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.34"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{studentFeedbackOneVM.studentFeedbackOneModel.txtDate}"
                        style="@style/txtInterextrabold15" tools:text="@string/lbl_date"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt4232023"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_135pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.66"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{studentFeedbackOneVM.studentFeedbackOneModel.txt4232023}"
                        style="@style/txtInterbold14"
                        textUnderline="true" tools:text="@string/lbl_4_23_2023"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowinstructor"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtInstructor"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="0.31"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{studentFeedbackOneVM.studentFeedbackOneModel.txtInstructor}"
                        style="@style/txtInterextrabold17" tools:text="@string/lbl_instructor"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnName"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_28pxh"
                        android:layout_weight="0.69"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_name2"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_5" tools:text="@string/lbl_name2"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{studentFeedbackOneVM.studentFeedbackOneModel.txtDescription}"
                    style="@style/txtInterextrabold17" tools:text="@string/lbl_description2"/>
                <View android:background="@color/white_A700"
                    android:id="@+id/viewRectangleTwentyTwo"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_141pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_320pxh"/>
                <LinearLayout android:id="@+id/linearRowactiontaken"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtActionTaken"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_weight="0.39"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{studentFeedbackOneVM.studentFeedbackOneModel.txtActionTaken}"
                        style="@style/txtInterextrabold17" tools:text="@string/lbl_action_taken"/>
                    <LinearLayout
                        android:id="@+id/linearRowgiveawarning"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_weight="0.61"
                        android:layout_width="0dp"
                        android:orientation="horizontal" style="@style/groupStylewhite_5">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtGiveawarning"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginStart="@dimen/_30pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_weight="0.92"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{studentFeedbackOneVM.studentFeedbackOneModel.txtGiveawarning}"
                            style="@style/txtIntersemibold14_1" tools:text="@string/lbl_give_a_warning"/>
                        <ImageView android:id="@+id/imageArrowup"
                            android:layout_height="@dimen/_5pxh"
                            android:layout_marginBottom="@dimen/_9pxv"
                            android:layout_marginEnd="@dimen/_20pxh"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_marginTop="@dimen/_15pxv"
                            android:layout_width="@dimen/_8pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowup"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowup"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowstudentscase"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtStudentsCase"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_63pxv"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="0.42"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{studentFeedbackOneVM.studentFeedbackOneModel.txtStudentsCase}"
                        style="@style/txtInterextrabold17" tools:text="@string/lbl_student_s_case"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnlabel"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_weight="0.58"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStylewhite_A700_b2cornerRadius">
                        <androidx.appcompat.widget.AppCompatCheckBox
                            android:buttonTint="@color/gray_700"
                            android:ellipsize="end"
                            android:fontFamily="@font/inter"
                            android:id="@+id/checkBoxLabel"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_17pxv"
                            android:layout_width="wrap_content"
                            android:paddingStart="@dimen/_9pxh"
                            android:paddingTop="@dimen/_2pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_bullying"
                            android:textColor="@color/gray_700"
                            android:textFontWeight="600"
                            android:textSize="@dimen/_16pxh"
                            android:textStyle="bold" tools:text="@string/lbl_bullying"/>
                        <androidx.appcompat.widget.AppCompatCheckBox
                            android:buttonTint="@color/gray_700"
                            android:ellipsize="end"
                            android:fontFamily="@font/inter"
                            android:id="@+id/checkBoxLabelOne"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_18pxv"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_17pxv"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_1pxv"
                            android:paddingStart="@dimen/_9pxh"
                            android:paddingTop="@dimen/_1pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_bullied"
                            android:textColor="@color/gray_700"
                            android:textFontWeight="600"
                            android:textSize="@dimen/_16pxh"
                            android:textStyle="bold" tools:text="@string/lbl_bullied"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnSave"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_marginEnd="@dimen/_5pxh"
                android:layout_marginStart="@dimen/_181pxh"
                android:layout_marginTop="@dimen/_34pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_9pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_9pxv"
                android:singleLine="true"
                android:text="@string/lbl_save"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_6" tools:text="@string/lbl_save"/>
        </LinearLayout>
    </FrameLayout>
</layout>
