<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout android:gravity="start" android:id="@+id/linearForm"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <TextView android:ellipsize="end" android:id="@+id/txtUsername"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_118pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:singleLine="true"
            style="@style/txtInterregular13" tools:text="@string/lbl_username"/>
        <EditText
            android:background="@drawable/rectangle_bg_white_a700_border_deep_orange_202_radius_5"
            android:ellipsize="end" android:hint="@string/lbl_username"
            android:id="@+id/etFrame" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:paddingBottom="@dimen/_14pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_15pxh"
            android:paddingTop="@dimen/_15pxv" android:singleLine="true"
            android:textColorHint="@color/deep_orange_A101"
            style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtDescription"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_42pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:singleLine="true"
            style="@style/txtInterregular13" tools:text="@string/lbl_description"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnnameOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_4">
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtNameOne"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_91pxv"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:singleLine="true"
                style="@style/txtInterregular13_1" tools:text="@string/lbl_description"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatCheckBox
            android:buttonTint="@color/deep_orange_202"
            android:ellipsize="end" android:fontFamily="@font/inter"
            android:id="@+id/checkBoxBullying"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_36pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:paddingStart="@dimen/_14pxh"
            android:paddingTop="@dimen/_1pxv" android:singleLine="true"
            android:text="@string/lbl_bullying"
            android:textColor="@color/black_900"
            android:textFontWeight="400"
            android:textSize="@dimen/_13pxh" android:textStyle="normal" tools:text="@string/lbl_bullying"/>
        <androidx.appcompat.widget.AppCompatCheckBox
            android:buttonTint="@color/deep_orange_202"
            android:ellipsize="end" android:fontFamily="@font/inter"
            android:id="@+id/checkBoxBullied"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:paddingStart="@dimen/_14pxh"
            android:singleLine="true" android:text="@string/lbl_bullied"
            android:textColor="@color/black_900"
            android:textFontWeight="400"
            android:textSize="@dimen/_13pxh" android:textStyle="normal" tools:text="@string/lbl_bullied"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnSave"
            android:layout_height="@dimen/_43pxv"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_37pxh"
            android:layout_marginStart="@dimen/_37pxh"
            android:layout_marginTop="@dimen/_71pxv"
            android:layout_width="@dimen/_68pxh"
            android:lineSpacingExtra="@dimen/_4pxh"
            android:paddingBottom="@dimen/_12pxv"
            android:paddingEnd="@dimen/_18pxh"
            android:paddingStart="@dimen/_15pxh"
            android:paddingTop="@dimen/_12pxv" android:singleLine="true"
            android:text="@string/lbl_save" android:textAllCaps="false"
            style="@style/btnSolidRoundedOutline_1" tools:text="@string/lbl_save"/>
    </LinearLayout>
</layout>
