<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:andriod="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameLoginPage"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    tools:context=".ActivityStudentList">

    <LinearLayout
        android:id="@+id/linearRowarrowleft"
        style="@style/groupStyledeep_orange_A400_d6"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_90pxh"
        android:layout_marginBottom="-13dp">

        <ImageView
            android:id="@+id/imageArrowleft"
            android:layout_width="@dimen/_24pxh"
            android:layout_height="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_marginBottom="@dimen/_94pxv"
            android:scaleType="fitXY"
            android:onClick="goBackToParent"
            android:src="@drawable/img_arrowleft"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/txtGrade6Secti"
            style="@style/txtInterextrabold18_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_55pxh"
            android:layout_marginTop="@dimen/_34pxv"
            android:ellipsize="end"
            android:lineSpacingExtra="@dimen/_4pxh"
            android:singleLine="true"
            android:text="@string/msg_grade_6_secti" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearColumnsearch"
        android:layout_width="237dp"
        android:layout_height="46dp"
        android:layout_below="@+id/linearRowarrowleft"
        android:layout_marginStart="@dimen/_67pxh"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="@dimen/_19pxh"
        android:background="@color/gray_401">

        <ImageView
            android:id="@+id/imageSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_10pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_search"
            tools:ignore="ContentDescription" />

        <EditText
            android:id="@+id/searchStudents"
            style="@style/txtInterregular15"
            android:layout_width="176dp"
            android:layout_height="40dp"
            android:layout_marginStart="@dimen/_2pxh"
            android:layout_marginTop="@dimen/_1pxh"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginBottom="@dimen/_9pxv"
            android:ellipsize="end"
            android:hint="@string/msg_search_for_stud"
            android:inputType="textShortMessage"
            android:lineSpacingExtra="@dimen/_4pxh"
            android:singleLine="true" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearColumnlogin"
        style="@style/groupStylewhite_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearColumnsearch"
        android:layout_gravity="top|center"
        android:layout_marginStart="@dimen/_16pxh"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="@dimen/_16pxh"
        android:layout_marginBottom="58dp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearRowlogin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_36pxh"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/students"
                style="@style/txtInterextrabold12"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.44"
                android:background="@android:color/transparent"
                android:ellipsize="end"
                android:singleLine="true"
                android:onClick="goToStudentList"
                android:text="Students"
                tools:ignore="TextSizeCheck,TextSizeCheck" />


            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/report"
                style="@style/txtInterextrabold12_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_70pxh"
                android:layout_weight="0.44"
                android:background="@android:color/transparent"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="Report"
                android:onClick="goToReport"
                tools:ignore="TextSizeCheck" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/txtTaptoviewins"
        style="@style/txtInterbold14"
        android:layout_width="@dimen/_169pxh"
        android:layout_height="wrap_content"
        android:layout_marginStart="124dp"
        android:layout_marginEnd="105dp"
        android:layout_marginTop="@dimen/_199pxh"
        android:singleLine="true"
        android:text="Name of Students" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_219pxh"
        android:layout_marginBottom="@dimen/_119pxh">

        <ListView
            android:id="@+id/namesListView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>


    </RelativeLayout>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnClassEvaluationOne"
        style="@style/btnSolidRounded_3"
        textUnderline="true"
        android:layout_width="@dimen/_195pxh"
        android:layout_height="@dimen/_29pxv"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="91dp"
        android:layout_marginTop="701dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="67dp"
        android:ellipsize="end"
        android:gravity="center"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_11pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingBottom="@dimen/_11pxv"
        android:singleLine="true"
        android:text="@string/msg_class_evaluatio"
        android:textAllCaps="false"
        tools:text="@string/msg_class_evaluatio" />

</RelativeLayout>
