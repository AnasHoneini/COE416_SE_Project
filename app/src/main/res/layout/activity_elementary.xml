<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:andriod="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameLoginPage"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    tools:context=".ActivityElementary">

    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearGradeSectionKG"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        style="@style/groupStylewhite_A700">

        <FrameLayout
            android:layout_gravity="top|start"
            android:id="@+id/frameStackrectanglefive"
            android:layout_height="@dimen/_152pxv"
            android:layout_width="match_parent">

            <ImageView android:id="@+id/imageRectangleFive"
                android:layout_gravity="start"
                android:layout_height="@dimen/_120pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                tools:ignore="ContentDescription"
                android:src="@drawable/img_rectangle5_152x360"/>


            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                style="@style/groupStyledeep_orange_A400_d6">

                <ImageView
                    android:id="@+id/imageArrowleft"
                    android:layout_width="@dimen/_24pxh"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_marginBottom="@dimen/_111pxv"
                    android:scaleType="fitXY"
                    android:onClick="goBackToDashboard"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" />

                <FrameLayout
                    android:id="@+id/frameStackellipsetwo"
                    android:layout_width="@dimen/_202pxh"
                    android:layout_height="91dp"
                    android:layout_marginStart="@dimen/_120pxh"
                    android:layout_marginBottom="@dimen/_63pxv">

                    <LinearLayout
                        android:id="@+id/linearColumnwelcomeuser"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|end"
                        android:layout_marginStart="@dimen/_50pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_6pxh"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txtWelcomeuser"
                            style="@style/txtInterregular18"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:ellipsize="end"
                            android:gravity="start"
                            android:lineSpacingExtra="@dimen/_4pxh"
                            android:singleLine="true" />

                        <TextClock
                            android:id="@+id/textClock"
                            android:layout_width="@dimen/_150pxh"
                            android:layout_height="@dimen/_40pxh"
                            android:layout_marginTop="@dimen/_8pxh"
                            android:textSize="24sp" />

                    </LinearLayout>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/linearRowGradesSixAndBelow"
            style="@style/btnSolidRounded_2"
            android:layout_height="48dp"
            android:layout_marginEnd="@dimen/_39pxh"
            android:layout_marginStart="@dimen/_39pxh"
            android:layout_marginTop="@dimen/_170pxh"
            android:layout_width="match_parent"
            android:gravity="center"
            android:ellipsize="end"
            android:singleLine="true"
            android:onClick="goToGrades16"
            android:textSize="@dimen/_18pxh"
            tools:ignore="SpUsage,TextSizeCheck"
            android:text="@string/lbl_grades_1_6"/>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/LinearRowGradesSevenNine"
            style="@style/btnSolidRounded_2"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginStart="@dimen/_39pxh"
            android:layout_marginTop="@dimen/_150pxh"
            android:layout_marginEnd="@dimen/_39pxh"
            android:ellipsize="end"
            android:onClick="goToGrades79"
            android:gravity="center"
            android:textSize="@dimen/_18pxh"
            android:singleLine="true"
            tools:ignore="TextSizeCheck"
            android:text="@string/lbl_grades_7_9" />

    </LinearLayout>
</RelativeLayout>
