<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:andriod="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameLoginPage"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    tools:context=".ActivityElementary">

    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearGradeSectionKG"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        style="@style/groupStylewhite_A700">

        <FrameLayout
            android:layout_gravity="top|start"
            android:id="@+id/frameStackrectanglefive"
            android:layout_height="@dimen/_258pxv"
            android:layout_width="match_parent">

            <ImageView
                android:id="@+id/imageRectangleFive"
                android:layout_width="match_parent"
                android:layout_height="206dp"
                android:layout_gravity="start"
                android:scaleType="fitXY"
                android:src="@drawable/img"
                tools:ignore="ContentDescription" />


            <LinearLayout
                android:id="@+id/linearRowarrowleft"
                android:layout_width="match_parent"
                android:layout_height="229dp"
                android:layout_gravity="start"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageArrowleft"
                    android:layout_width="@dimen/_24pxh"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_marginBottom="@dimen/_111pxv"
                    android:onClick="goBackToDashboard"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" />

            </LinearLayout>
        </FrameLayout>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/linearRowGradesSixAndBelow"
            style="@style/btnSolidRounded_2"
            android:layout_height="48dp"
            android:layout_marginEnd="@dimen/_39pxh"
            android:layout_marginStart="@dimen/_39pxh"
            android:layout_marginTop="@dimen/_100pxh"
            android:layout_width="match_parent"
            android:gravity="center"
            android:ellipsize="end"
            android:singleLine="true"
            android:onClick="goToGrades16"
            android:textSize="@dimen/_18pxh"
            tools:ignore="SpUsage,TextSizeCheck"
            android:text="@string/lbl_grades_1_6"/>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/LinearRowGradesSevenNine"
            style="@style/btnSolidRounded_2"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginStart="@dimen/_39pxh"
            android:layout_marginTop="@dimen/_120pxh"
            android:layout_marginEnd="@dimen/_39pxh"
            android:ellipsize="end"
            android:onClick="goToGrades79"
            android:gravity="center"
            android:textSize="@dimen/_18pxh"
            android:singleLine="true"
            tools:ignore="TextSizeCheck"
            android:text="@string/lbl_grades_7_9" />

    </LinearLayout>
</RelativeLayout>
