<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:andriod="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameLoginPage"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    tools:context=".ActivityDashboard">

    <LinearLayout
        android:id="@+id/linearColumnsearch"
        style="@style/groupStylebluegray_101"
        android:layout_width="match_parent"
        android:layout_height="550dp"
        android:layout_below="@+id/linearColumnno"
        android:layout_gravity="bottom|start"
        android:layout_marginTop="-24dp"
        android:orientation="vertical">

        <FrameLayout
            android:id="@+id/frameStackarrowright"
            android:layout_width="@dimen/_270pxh"
            android:layout_height="@dimen/_61pxv"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_37pxv"
            android:layout_marginEnd="@dimen/_39pxh">

            <LinearLayout
                android:id="@+id/linearRowkindergarten"
                style="@style/groupStylewhite_A700cornerRadius"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/txtKindergarten"
                    style="@style/txtInterextrabold18"
                    android:layout_width="0dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="@dimen/_71pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_marginBottom="@dimen/_21pxv"
                    android:layout_weight="0.94"
                    android:background="@android:color/transparent"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:onClick="goToKindergarten"
                    android:singleLine="true"
                    android:text="@string/lbl_kindergarten"
                    android:textAllCaps="false"
                    android:textSize="@dimen/_17pxh"
                    tools:ignore="SpUsage,TextSizeCheck" />

                <ImageView
                    android:id="@+id/imageArrowrightOne"
                    android:layout_width="@dimen/_7pxh"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_marginEnd="@dimen/_55pxh"
                    android:layout_marginBottom="@dimen/_27pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_deep_orange_a401"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_arrowright_deep_orange_a401" />
            </LinearLayout>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/frameStackarrowrightTwo"
            android:layout_width="@dimen/_270pxh"
            android:layout_height="@dimen/_61pxv"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_35pxv"
            android:layout_marginEnd="@dimen/_39pxh">

            <LinearLayout
                android:id="@+id/linearRowelementary"
                style="@style/groupStylewhite_A700cornerRadius"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/txtElementary"
                    style="@style/txtInterextrabold18"
                    android:layout_width="0dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="@dimen/_71pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_marginBottom="@dimen/_21pxv"
                    android:layout_weight="0.94"
                    android:background="@android:color/transparent"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:onClick="goToElementary"
                    android:singleLine="true"
                    android:text="@string/lbl_elementary"
                    android:textAllCaps="false"
                    android:textSize="@dimen/_17pxh"
                    tools:ignore="SpUsage,TextSizeCheck" />

                <ImageView
                    android:id="@+id/imageArrowrightThree"
                    android:layout_width="@dimen/_7pxh"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_marginEnd="@dimen/_57pxh"
                    android:layout_marginBottom="@dimen/_22pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_deep_orange_a401"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_arrowright_deep_orange_a401" />
            </LinearLayout>
        </FrameLayout>

        <LinearLayout
            android:id="@+id/linearRowhighschool"
            style="@style/groupStylewhite_A700cornerRadius"
            android:layout_width="match_parent"
            android:layout_height="66dp"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_31pxv"
            android:layout_marginEnd="@dimen/_39pxh"
            android:layout_marginBottom="@dimen/_33pxv"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/txtHighSchool"
                style="@style/txtInterextrabold18"
                android:layout_width="0dp"
                android:layout_height="48dp"
                android:layout_marginStart="@dimen/_71pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_marginBottom="@dimen/_21pxv"
                android:layout_weight="0.94"
                android:background="@android:color/transparent"
                android:ellipsize="end"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:onClick="goToHighSchool"
                android:singleLine="true"
                android:text="@string/lbl_high_school"
                android:textAllCaps="false"
                android:textSize="@dimen/_17pxh"
                tools:ignore="SpUsage,TextSizeCheck" />

            <ImageView
                android:id="@+id/imageArrowrightFour"
                android:layout_width="@dimen/_7pxh"
                android:layout_height="@dimen/_12pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_26pxv"
                android:layout_marginEnd="@dimen/_57pxh"
                android:layout_marginBottom="@dimen/_26pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowright_deep_orange_a401"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_arrowright_deep_orange_a401" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearColumnno"
        style="@style/groupStyledeep_orange_200cornerRadius"
        android:layout_width="match_parent"
        android:layout_height="198dp"
        android:layout_gravity="top|start"
        android:layout_marginTop="@dimen/_222pxv"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearRowno"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_23pxh"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txtNo"
                style="@style/txtInterbold16"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_weight="0.14"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_no" />

            <TextView
                android:id="@+id/txtIncident"
                style="@style/txtIntersemibold16"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_weight="0.33"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="Incident" />

            <TextView
                android:id="@+id/txtTime"
                style="@style/txtIntersemibold16"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_41pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_weight="0.21"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_time" />

            <TextView
                android:id="@+id/txtStudent"
                style="@style/txtIntersemibold16"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_42pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_weight="0.33"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_student" />
        </LinearLayout>

        <View
            android:id="@+id/lineLineSix"
            android:layout_width="@dimen/_355pxh"
            android:layout_height="@dimen/_1pxv"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_5pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:background="@color/gray_600" />

        <FrameLayout
            android:id="@+id/linearRowlanguageTwo"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:gravity="end"
            android:orientation="horizontal" />

    </LinearLayout>

    <ImageView
        android:id="@+id/imageRectangleFive"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_220pxh"
        android:layout_gravity="start"
        android:scaleType="fitXY"
        android:src="@drawable/img_1" />


    <TextView
        android:id="@+id/txtWelcomeuser"
        style="@style/txtInterregular18"
        android:layout_width="163dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_220pxh"
        android:layout_marginTop="@dimen/_8pxh"
        android:layout_marginEnd="@dimen/_10pxh"
        android:ellipsize="end"
        android:background="#80FFFFFF"
        android:gravity="start"
        android:lineSpacingExtra="@dimen/_4pxh"
        android:singleLine="true"
        tools:ignore="TextSizeCheck" />

    <TextView
            android:id="@+id/btnInboxFour"
            style="@style/btnSolidRounded_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="top|center"
            android:layout_marginStart="@dimen/_65pxh"
            android:layout_marginTop="@dimen/_195pxv"
            android:layout_marginEnd="@dimen/_65pxh"
            android:layout_marginBottom="@dimen/_202pxv"
            android:ellipsize="end"
            android:gravity="center"
            android:lineSpacingExtra="@dimen/_4pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_6pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingBottom="@dimen/_6pxv"
            android:singleLine="true"
            android:textAllCaps="false"
            tools:ignore="TextSizeCheck"
            android:text="inbox: "/>
    </RelativeLayout>

